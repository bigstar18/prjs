<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
<!--注意：VALUE不能为空，不然会报解析不了XML的错误 -->
    <COMPONENT>
        <NAME>BANK</NAME>
        <TYPE>normal</TYPE> 
		<SECTION>
            <NAME>Processor</NAME>
             <PROPERTYS>
                <!-- 处理器数据库类型 -->
				<PROPERTY>
                    <KEY>DBType</KEY>
                    <VALUE>Oracle</VALUE>
                </PROPERTY>
				<!-- DBUrl -->
				<PROPERTY>
                    <KEY>DBUrl</KEY>
                    <VALUE>jdbc:oracle:thin:@10.0.100.183:1521:gnnt</VALUE>
                </PROPERTY>
				<!-- DBUser -->
				<PROPERTY>
                    <KEY>DBUser</KEY>
                    <VALUE>trade_gnnt</VALUE>
                </PROPERTY>
				<!-- DBPassword -->
				<PROPERTY>
                    <KEY>DBPassword</KEY>
                    <VALUE>password</VALUE>
                </PROPERTY>
				<!-- 数据源 -->
				<PROPERTY>
						<KEY>JNDIName</KEY>
						<VALUE>java:/comp/env/mgr</VALUE>
				</PROPERTY>
				<!-- 数据库连接类型 -->
				<PROPERTY>
						<KEY>DBConnType</KEY>
						<VALUE>JNDIName</VALUE>
				</PROPERTY>
				<!--处理器远程IP地址 -->
				<PROPERTY>
					<KEY>RmiIpAddress</KEY> 
					<VALUE>10.0.100.181</VALUE>
				</PROPERTY>
				<!--远程端口 -->
				<PROPERTY>
					<KEY>RmiPortNumber</KEY> 
					<VALUE>4117</VALUE>
				</PROPERTY>
				<!--数据端口 -->
				<PROPERTY>
					<KEY>RmiDataPort</KEY> 
					<VALUE>4118</VALUE>
				</PROPERTY>
				<!--远程服务器名称 -->
				<PROPERTY>
					<KEY>RmiServiceName</KEY> 
					<VALUE>ComplexProcessorRMI</VALUE>
				</PROPERTY>
				<!-- 交易系统编号(模块号) -->
				<PROPERTY>
                    <KEY>SystemID</KEY>
                    <VALUE>1001</VALUE>
                </PROPERTY>
                <!-- 平台处理器RMI链接 -->
				<PROPERTY>
                    <KEY>platformURL</KEY>
                    <VALUE>//10.0.100.181:2118/PlatformProcessorRMI</VALUE>
                </PROPERTY>
            </PROPERTYS>
        </SECTION>
    </COMPONENT>
</CONFIG>
