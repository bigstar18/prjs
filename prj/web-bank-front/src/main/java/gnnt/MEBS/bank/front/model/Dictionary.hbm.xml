<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping>
	<class name="gnnt.MEBS.bank.front.model.Dictionary" table="F_B_DICTIONARY" dynamic-update="true" lazy="false">
		<id name="id"/>

		<property name="type"/>
		<property name="name"/>
		<property name="value"/>
		<property name="note"/>
		<property name="bankID"/>
		
		<many-to-one name="bank" class="gnnt.MEBS.bank.front.model.Bank" column="bankID" insert="false" update="false"></many-to-one>
	</class>
</hibernate-mapping>

