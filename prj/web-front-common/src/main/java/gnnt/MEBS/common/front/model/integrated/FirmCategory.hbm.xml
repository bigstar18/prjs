<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping package="gnnt.MEBS.common.front.model.integrated">
	<class name="FirmCategory" table="M_FirmCategory" dynamic-update="true" lazy="false">
		<id name="id">
			<generator class="assigned">
				<param name="sequence">SEQ_M_FIRMCATEGORY</param>
			</generator>
		</id>
		<property name="name"></property>
		<property name="isvisibal"></property>
		<property name="sortNo"></property>
		<property name="note"></property>
	</class>
</hibernate-mapping>

