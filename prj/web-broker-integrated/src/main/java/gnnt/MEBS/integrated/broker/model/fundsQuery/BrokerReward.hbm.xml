<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping>
	<class name="gnnt.MEBS.integrated.broker.model.fundsQuery.BrokerReward" table="BR_BROKERREWARD">
		<composite-id>
			<key-property name="brokerId"></key-property>
			<key-property name="occurDate"></key-property>
			<key-property name="moduleId"></key-property>
		</composite-id>
		<property name="amount"></property>
		<property name="paidAmount"></property>
		<property name="payDate"></property>
	</class>
</hibernate-mapping>
