<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gnnt.MEBS.commodity.model">
	<class name="Commoditytradeprop" table="T_A_COMMODITYTRADEPROP" dynamic-update="true">
	
		<composite-id name="myId" class="CommoditytradepropKey">
			<key-property name="commidityid" column="COMMODITYID"></key-property>
			<key-property name="sectionid" column="SECTIONID"></key-property>
		</composite-id>
		<property name="modifytime"></property>
	</class>
</hibernate-mapping>
