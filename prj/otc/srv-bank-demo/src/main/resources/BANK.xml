<?xml version="1.0" encoding="GBK"?>
<CONFIG>
<!--注意：VALUE不能为空，不然会报解析不了XML的错误 -->
    <COMPONENT>
        <NAME>BANK</NAME>
        <TYPE>normal</TYPE> 
		<SECTION>
            <NAME>Processor</NAME>
             <PROPERTYS>
                <!-- 处理器数据库类型 -->
				<PROPERTY>
                    <KEY>DBType</KEY>
                    <VALUE>Oracle</VALUE>
                </PROPERTY>
				<!-- DBUrl -->
				<PROPERTY>
                    <KEY>DBUrl</KEY>
                    <VALUE>jdbc:oracle:thin:@10.10.10.246:1521:oracle</VALUE>
                </PROPERTY>
				<!-- DBUser -->
				<PROPERTY>
                    <KEY>DBUser</KEY>
                    <VALUE>pmes003</VALUE>
                </PROPERTY>
				<!-- DBPassword -->
				<PROPERTY>
                    <KEY>DBPassword</KEY>
                    <VALUE>password</VALUE>
                </PROPERTY>
				<!-- 数据源 -->
				<PROPERTY>
					<KEY>JNDIName</KEY>
					<VALUE>java:/comp/env/interface_proccessor</VALUE>
				</PROPERTY>
				<!-- 数据库连接类型 -->
				<PROPERTY>
					<KEY>DBConnType</KEY>
					<VALUE>JNDIName</VALUE>
				</PROPERTY>
				<!--处理器远程IP地址 -->
				<PROPERTY>
					<KEY>RmiIpAddress</KEY> 
					<VALUE>10.10.10.215</VALUE>
				</PROPERTY>
				<!--远程端口 -->
				<PROPERTY>
					<KEY>RmiPortNumber</KEY>
					<VALUE>31000</VALUE>
				</PROPERTY>
				<!--远程服务器名称 -->
				<PROPERTY>
					<KEY>RmiServiceName</KEY> 
					<VALUE>CapitalProcessorRMI</VALUE>
				</PROPERTY>
				<!-- 初始资金密码 -->
				<PROPERTY>
					<KEY>password</KEY>
					<VALUE>111111</VALUE>
				</PROPERTY>
				<!-- 轮询判断是否生成清算文件的时间间隔(单位为分钟)  -->
				<PROPERTY>
					<KEY>qssleeptime</KEY>
					<VALUE>5</VALUE>
				</PROPERTY>
				<!-- 光大银行生成的清算文件路径  -->
				<PROPERTY>
					<KEY>cebqsdir</KEY>
					<VALUE>/home/smoke002/</VALUE>
				</PROPERTY>
				<!--设置可以对账时间 -->
				<PROPERTY>
					<KEY>CompareTime</KEY> 
					<VALUE>15:30:00</VALUE>
				</PROPERTY>
				<!-- 当银行账号为空时的默认值 -->
				<PROPERTY>
                    <KEY>DefaultAccount</KEY>
                    <VALUE>999999999999999999</VALUE>
                </PROPERTY>
            </PROPERTYS>
        </SECTION>
        
    </COMPONENT>
</CONFIG>
