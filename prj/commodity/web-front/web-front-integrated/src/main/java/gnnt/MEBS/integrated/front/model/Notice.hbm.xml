<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hebernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gnnt.MEBS.integrated.front.model">
    <class name="Notice" table="M_Notice" dynamic-update="true" lazy="false">
        <id name="NoticeId"/>

        <property name="title"/> 
        <property name="content"/>
        <property name="createTime" insert="false" update="false"/> 
        <property name="userId"/>     
        
    </class>
</hibernate-mapping>