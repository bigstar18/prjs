<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping>
	<class name="gnnt.MEBS.broker.mgr.model.brokerManagement.Firm" table="M_FIRM">
		<id name="firmId">
		</id>
		<property name="name"></property>
		<property name="fullName"></property>
		<property name="type"></property>
		<property name="contactMan"></property>
		<property name="certificateType"></property>
		<property name="certificateNO"></property>
		<property name="phone"></property>
		<property name="mobile"></property>
		<property name="fax"></property>
		<property name="address"></property>
		<property name="postCode"></property>
		<property name="email"></property>
		<property name="zoneCode"></property>
		<property name="industryCode"></property>
		<property name="organizationCode"></property>
		<property name="corporateRepresentative"></property>
		<property name="note"></property>
		<property name="extendData"></property>
		<property name="createTime"></property>
		<property name="modifyTime"></property>
		<property name="status"></property>
		
		
	</class>
</hibernate-mapping>
