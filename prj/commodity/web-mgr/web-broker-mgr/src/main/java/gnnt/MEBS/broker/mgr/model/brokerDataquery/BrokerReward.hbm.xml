<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping>
	<class name="gnnt.MEBS.broker.mgr.model.brokerDataquery.BrokerReward" table="BR_BROKERREWARD">
		<composite-id>
		  <key-property name="brokerId" column="BROKERID"/>
		  <key-property name="occurDate"/>
		  <key-property name="moduleId" column="MODULEID"/>
		</composite-id>
		<property name="amount" />
		<property name="paidAmount" />
		<property name="payDate" />
			
	</class>
</hibernate-mapping>
