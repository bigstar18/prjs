<?xml version="1.0" encoding="GBK" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="ajaxcheck" extends="json-default" namespace="/ajaxcheck">
	<!-- ==仓单录入 交易商代码== -->
		<!-- 鼠标移开事件检查交易商代码在交易商列表表(M_Firm)表中是否存在(存在返回失败，不存在返回成功) -->
		<action name="checkStockByFirmId" class="ajaxCheck"
			method="checkStockByFirmId">
			<result name="success" type="json">
				<param name="root">jsonValidateReturn</param>
			</result>
		</action>
		
		
		<!-- 鼠标移开事件检查密钥是否正确(不正确返回失败，正确返回成功) hxx -->
		<action name="checkKey" class="ajaxCheck"
			method="checkKey">
			<result name="success" type="json">
				<param name="root">jsonValidateReturn</param> 
			</result>
		</action>
		<!-- Form 表单提交事件检查交易商代码在交易商列表表(M_Firm)表中是否存在(存在返回失败，不存在返回成功 -->
		<action name="checkStockFirmIdForm" class="ajaxCheck" method="checkStockFirmIdForm">
			<result name="success" type="json">
				<param name="root">jsonValidateReturn</param>
			</result>
		</action>
		
	<!-- ==仓库管理== -->
		<!-- 鼠标移开事件检查仓库编号在仓库表(BI_Warehouse)表中是否存在(存在返回失败，不存在返回成功) -->
		<action name="checkWarehouseBywarehouseId" class="ajaxCheck"
			method="checkWarehouseBywarehouseId">
			<result name="success" type="json">
				<param name="root">jsonValidateReturn</param>
			</result>
		</action>
		<!-- Form 表单提交事件检查仓库编号在仓库表(BI_Warehouse)表中是否存在(存在返回失败，不存在返回成功) -->
		<action name="checkWarehouseForm" class="ajaxCheck" method="checkWarehouseForm">
			<result name="success" type="json">
				<param name="root">jsonValidateReturn</param>
			</result>
		</action>
		
		<!-- 鼠标移开事件检查后台管理员代码在管理员表(W_USER)是否存在(存在返回失败，不存在返回成功) -->
		<action name="checkWuserByUserId" class="ajaxCheck"
			method="checkWuserByUserId">
			<result name="success" type="json">
				<param name="root">jsonValidateReturn</param>
			</result>
		</action>
	<!-- Form 表单提交事件检查后台管理员代码在管理员表(W_USER)是否存在(存在返回失败，不存在返回成功) -->
		<action name="checkWuserForm" class="ajaxCheck"
			method="checkWuserForm">
			<result name="success" type="json">
				<param name="root">jsonValidateReturn</param>
			</result>
		</action>
	</package>
</struts>