<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!--
		通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
	-->
<hibernate-mapping>
	<class name="gnnt.MEBS.timebargain.mgr.model.settle.Stock" table="BI_STOCK" dynamic-update="true" lazy="false">
		<id name="stockID">
			<generator class="native">
				<param name="sequence">SEQ_BI_STOCK</param>
			</generator>
		</id>
		<property name="realStockCode"/>
		<property name="breedID" />
		<property name="warehouseID"/>
		<property name="quantity"/>
		<property name="unit" />
		<property name="ownerFirm" />
		<property name="stockStatus" />
		
	</class>
</hibernate-mapping>
