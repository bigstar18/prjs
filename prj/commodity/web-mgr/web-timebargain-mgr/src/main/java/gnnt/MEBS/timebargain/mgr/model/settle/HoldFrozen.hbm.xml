<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping>
	<class name="gnnt.MEBS.timebargain.mgr.model.settle.HoldFrozen" table="T_Holdfrozen" dynamic-update="true" lazy="false">
		<id name="id">
			<generator class="native">
				<param name="sequence">SEQ_T_Holdfrozen</param>
			</generator>
		</id>
		<property name="operation" />
		<property name="firmId" />
		<property name="commodityId" />
		<property name="bs_Flag" />
		<property name="frozentype" />
		<property name="frozenQty" />
		<property name="frozenTime" />
		<property name="customerId" />
	</class>
</hibernate-mapping>