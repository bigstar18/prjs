<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!--
		通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
	-->
<hibernate-mapping>
	<class name="gnnt.MEBS.timebargain.mgr.model.settle.CommodityS" table="T_COMMODITY" dynamic-update="true" lazy="false">
		<id name="commodityId">
			<column name="COMMODITYID" />
		</id>
		<property name="name"/>
		<property name="sortId" />
		<property name="status" column="STATUS" />
		<property name="contractFactor"/>
		<property name="breedId" />
	</class>
</hibernate-mapping>
