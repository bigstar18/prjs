<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping package="gnnt.MEBS.integrated.mgr.model.usermanage">
	<class name="MFirmModule" table="M_FirmModule" dynamic-update="true" lazy="false">
		<composite-id>
			 <key-property name="moduleId" column="moduleId"/> 
			  <key-many-to-one name="mfirm" column="firmId" class="MFirm"/>
		</composite-id>
		<property name="enabled"></property>
		
	</class>
</hibernate-mapping>
