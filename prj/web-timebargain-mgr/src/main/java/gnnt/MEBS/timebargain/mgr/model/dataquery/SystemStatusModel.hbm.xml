<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!--
		通过配置文件配置 Model 类与数据库表之间的字段联系
	   将本配置文件配置到 hibernate.cfg.xml 中
	-->
<hibernate-mapping package="gnnt.MEBS.timebargain.mgr.model.dataquery">
	<class name="SystemStatusModel" table="T_SYSTEMSTATUS" lazy="false">
		<id name="tradeDate">
		   <generator class="assigned"></generator>
		</id>
		<property name="status" />
		<property name="sectionId" />
		<property name="note" />
		<property name="recoverTime" />	
	</class>
</hibernate-mapping>
