<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping>
	<class name="gnnt.MEBS.timebargain.mgr.model.deduct.CustomerHoldSum" table="T_CustomerHoldSum" dynamic-update="true" lazy="false">
		<composite-id>
			<key-property name="customerId"></key-property>
			<key-property name="commodityId"></key-property>
			<key-property name="bs_Flag"></key-property>
		</composite-id>
		<property name="holdQty" />
		<property name="holdFunds" />
		<property name="floatingLoss" />
		<property name="evenPrice" />
		<property name="frozenQty" />
		<property name="holdMargin" />
		<property name="gageQty" />
		<property name="holdAssure" />
		<property name="firmId" />
		<property name="gageFrozenQty" />
	</class>
</hibernate-mapping>
