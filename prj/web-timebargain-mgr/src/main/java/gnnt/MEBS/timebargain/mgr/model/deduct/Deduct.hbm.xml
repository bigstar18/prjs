<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping>
	<class name="gnnt.MEBS.timebargain.mgr.model.deduct.Deduct" table="T_E_DeductPosition" dynamic-update="true" lazy="false">
		<id name="deductId">
			<generator class="native">
				<param name="sequence">SEQ_T_E_DEDUCTPOSITION</param>
			</generator>
		</id>
		<property name="deductDate" />
		<property name="commodityId" />
		<property name="deductStatus" />
		<property name="deductPrice" />
		<property name="loserBSFlag" />
		<property name="loserMode" />
		<property name="lossRate" />
		<property name="selfCounteract" />
		<property name="profitLvl1" />
		<property name="profitLvl2" />
		<property name="profitLvl3" />
		<property name="profitLvl4" />
		<property name="profitLvl5" />
		<property name="execTime" />
		<property name="alert" />
	</class>
</hibernate-mapping>
