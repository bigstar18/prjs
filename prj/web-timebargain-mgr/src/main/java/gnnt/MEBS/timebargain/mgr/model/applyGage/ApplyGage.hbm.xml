<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping>
	<class name="gnnt.MEBS.timebargain.mgr.model.applyGage.ApplyGage" table="T_E_ApplyGage" dynamic-update="true" lazy="false">
		<id name="applyId">
			<generator class="native">
				<param name="sequence">SEQ_T_E_APPLYGAGE</param>
			</generator>
		</id>
		<property name="commodityId" />
		<property name="firmId" />
		<property name="customerId" />
		<property name="quantity" />
		<property name="applyType" />
		<property name="status" />
		<property name="createTime" />
		<property name="creator" />
		<property name="remark1" />
		<property name="modifyTime" />
		<property name="modifier" />
		<property name="remark2" />
	</class>
</hibernate-mapping>
