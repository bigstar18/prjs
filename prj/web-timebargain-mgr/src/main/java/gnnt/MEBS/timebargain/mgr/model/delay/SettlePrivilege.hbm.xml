<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping package="gnnt.MEBS.timebargain.mgr.model.delay">
	<class name="SettlePrivilege" table="T_A_SETTLEPRIVILEGE" dynamic-update="true">
		<id name="id">
		    <column name="ID" />
			<generator class="increment" />
		</id>
		<property name="type" />
		<property name="typeId" />
		<property name="kind" />
		<property name="kindId" />
		<property name="privilegecodeb" column="privilegecode_b" />
		<property name="privilegecodes" column="privilegecode_s" />
	</class>
</hibernate-mapping>
