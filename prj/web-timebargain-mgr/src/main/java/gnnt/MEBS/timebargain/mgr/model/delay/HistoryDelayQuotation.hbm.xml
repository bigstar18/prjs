<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping package="gnnt.MEBS.timebargain.mgr.model.delay">
	<class name="HistoryDelayQuotation" table="T_H_DelayQuotation" dynamic-update="true">
		<composite-id>  
		  <key-property name="commodityID" column="COMMODITYID"/>		  
		  <key-property name="clearDate" column="CLEARDATE"/>
		</composite-id>
		
		<property name="buySettleQty"/>
		<property name="sellSettleQty" />
		<property name="buyNeutralQty" />
		<property name="sellNeutralQty" />
		<property name="createTime" />
		<property name="neutralSide" />
		<property name="delayCleanHoldQty" />
	</class>
</hibernate-mapping>
