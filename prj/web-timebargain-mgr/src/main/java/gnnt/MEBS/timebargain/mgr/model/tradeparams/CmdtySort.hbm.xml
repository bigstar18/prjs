<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	通过配置文件配置 Model 类与数据库表之间的字段联系
	将本配置文件配置到 hibernate.cfg.xml 中
-->
<hibernate-mapping package="gnnt.MEBS.timebargain.mgr.model.tradeparams">
	<class name="CmdtySort" table="T_A_CmdtySort" dynamic-update="true" lazy="false">
		<id name="sortID" type="long">
		    <column name="sortID" />
			<generator class="increment">
			</generator>
		</id>
		<property name="sortName" />

	</class>
</hibernate-mapping>
