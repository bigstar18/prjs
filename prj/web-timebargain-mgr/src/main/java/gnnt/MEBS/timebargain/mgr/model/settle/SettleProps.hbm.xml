<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gnnt.MEBS.timebargain.mgr.model.settle">
	<class name="SettleProps" table="T_settleProps" dynamic-update="true"
		select-before-update="true" lazy="false">
		<id name="id" type="long" column="id">
			<generator class="native">
				<param name="sequence">SEQ_T_SETTLEPROPS</param>
			</generator>
		</id>
		<property name="commodityID" />
		<property name="propertyName" />
		<property name="propertyValue" />
		<property name="note" />
	</class>
</hibernate-mapping>